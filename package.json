{
  "name": "MediaBlock",
  "version": "1.0.0",
  "description": "* Good Company If a page is Good Company (TM), show its graphics.",
  "scripts": {
    "lint": "eslint **/*.jsx src/*/*.js",
    "setup": "bower install  # see also Makefile",
    "test": "yarn lint && yarn unittest",
    "unittest": "mocha",
    "dev:react": "babel --watch src/react --out-dir addon --presets react-app/prod",
    "dev:browserify": "browserify src/browserify/background.js -r ./src/browserify/whitelist.js -o addon/background.js",
    "dev:watchify": "watchify   src/browserify/background.js -r ./src/browserify/whitelist.js -o addon/background.js"
  },
  "repository": "git+https://github.com/serv-inc/MediaBlock.git",
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serv-inc/MediaBlock/issues"
  },
  "homepage": "https://github.com/serv-inc/MediaBlock#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "bower": "^1.8.8",
    "browserify": "^16.5.1",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "mocha": "^8.0.1",
    "prettier": "2.0.5",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "private": false
}
